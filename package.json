{
  "name": "stock-picker",
  "version": "1.0.0",
  "private": true,
  "description": "Stock picking and algorithmic trading platform",
  "scripts": {
    "dev": "pnpm --parallel --stream run dev",
    "build": "pnpm --stream -r run build",
    "test": "pnpm --stream -r run test",
    "clean": "pnpm --stream -r run clean",
    "typecheck": "pnpm --stream -r run typecheck",
    "lint": "pnpm --stream -r run lint",
    "migrate": "pnpm --filter @stock-picker/backend run migrate",
    "seed": "pnpm --filter @stock-picker/backend run seed",
    "deploy:staging": "pnpm --filter @stock-picker/infrastructure run deploy:staging",
    "deploy:prod": "pnpm --filter @stock-picker/infrastructure run deploy:prod"
  },
  "keywords": [
    "stock-picker",
    "algorithmic-trading",
    "backtesting"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
